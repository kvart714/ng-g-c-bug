{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "SchematicsAngularComponentWithStories",
  "title": "Angular Component With Stories",
  "type": "object",
  "description": "Generate component with .stories.ts",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the component.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the component?"
    }
  },
  "required": ["name"]
}
